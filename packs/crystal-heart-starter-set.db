":2,"price":null,"equippable":true,"equipStatus":3,"isArcaneDevice":false,"arcaneSkillDie":{"sides":4,"modifier":0},"powerPoints":{},"isVehicular":false,"mods":1,"actions":{"skill":"","skillMod":"","dmgMod":"","additional":{}},"favorite":false,"category":"","isAmmo":false,"equipped":false,"source":""},"ownership":{"default":0,"VCIFgtx8PqLPKqlv":3},"_stats":{"systemId":"swade","systemVersion":"2.1.7","coreVersion":"10.291","createdTime":1671035303362,"modifiedTime":1671037827461,"lastModifiedBy":"VCIFgtx8PqLPKqlv"},"folder":"lwcz46rIIuI88la5","sort":100000,"_id":"KnlzQiRebklM0rrX"},{"folder":"lwcz46rIIuI88la5","name":"Crystal Container","type":"gear","img":"modules/swade-crystal-heart/assets/arts/02-09-Spot-Starter-Crystal.webp","system":{"description":"<blockquote><p><span style=\"font-family: Signika, sans-serif\">A feral Crystal can be tamed when placed into a container.</span></p></blockquote><img src=\"modules/swade-crystal-heart/assets/arts/02-09-Spot-Starter-Crystal.webp\">","notes":"","additionalStats":{},"quantity":1,"weight":1,"price":0,"equippable":false,"equipStatus":1,"isArcaneDevice":false,"arcaneSkillDie":{"sides":4,"modifier":0},"powerPoints":{},"isVehicular":false,"mods":1,"actions":{"skill":"","skillMod":"","dmgMod":"","additional":{}},"favorite":false,"category":"","isAmmo":false,"source":""},"effects":[],"sort":0,"ownership":{"default":0,"VCIFgtx8PqLPKqlv":3},"flags":{"core":{"sourceId":"Item.SuT2vxbDTfCEuuiv"}},"_stats":{"systemId":"swade","systemVersion":"2.1.7","coreVersion":"10.291","createdTime":1671035081976,"modifiedTime":1671037487864,"lastModifiedBy":"VCIFgtx8PqLPKqlv"},"_id":"Cm11aATwunMWEhMu"},{"folder":"lwcz46rIIuI88la5","name":"Gloves","type":"gear","img":"icons/equipment/hand/glove-cuffed-studded-red.webp","system":{"description":"","notes":"","additionalStats":{},"quantity":1,"weight":0,"price":0,"equippable":false,"equipStatus":1,"isArcaneDevice":false,"arcaneSkillDie":{"sides":4,"modifier":0},"powerPoints":{},"isVehicular":false,"mods":1,"actions":{"skill":"","skillMod":"","dmgMod":"","additional":{}},"favorite":false,"category":"","isAmmo":false,"source":""},"effects":[],"sort":0,"ownership":{"default":0,"VCIFgtx8PqLPKqlv":3},"flags":{"core":{"sourceId":"Item.7K8lY4PeiMUz7Dfd"}},"_stats":{"systemId":"swade","systemVersion":"2.1.7","coreVersion":"10.291","createdTime":1671035207215,"modifiedTime":1671037833500,"lastModifiedBy":"VCIFgtx8PqLPKqlv"},"_id":"PF3r1pdrbccBD3Bz"},{"_id":"RPZJHgg6MszsaQPW","effects":[{"_id":"w9LmDZVB1VeibUQX","changes":[{"key":"system.initiative.hasQuick","mode":5,"value":"True","priority":null}],"disabled":false,"duration":{"startTime":null,"seconds":null,"combat":null,"rounds":null,"turns":null,"startRound":null,"startTurn":null},"flags":{},"icon":"/icons/svg/mystery-man.svg","label":"Quick","tint":null,"transfer":true,"origin":null}],"flags":{"cf":{"color":"#000000","id":"temp_neny3715hqg","path":"Edges#/CF_SEP/Background Edges"},"core":{"sourceId":"Compendium.swade-core-rules.swade-edges.H1xRqRutivEASOge"}},"folder":null,"img":"systems/swade/assets/icons/edge.svg","name":"Quick","sort":0,"type":"edge","system":{"description":"<article class=\"swade-core\">\n<p><span class=\"fontstyle0\">Quick characters have lightning-fast reflexes and a cool head. Whenever you are dealt an @Compendium[swade-core-rules.swade-rules.Action Cards (Initiative)]{Action Card} of Five or lower, you may discard it and draw again until you get a card higher than Five.</span></p>\n<p><span class=\"fontstyle0\">@UUID[Compendium.swade-core-rules.swade-rules.swadecor01charac.JournalEntryPage.01characters0000]{Characters} with both the @Compendium[swade-core-rules.swade-edges.Level Headed]{Level Headed} and Quick Edges first draw their additional card and choose which to take. If that card is a Five or less, the Quick Edge may be used to draw a replacement until it&rsquo;s Six or higher.</span></p>\n</article>","notes":"","additionalStats":{},"favorite":false,"category":"","isArcaneBackground":false,"requirements":{"value":"Novice, Agility d8+"},"source":""},"ownership":{"7ntJSLixH2Phv2jV":3,"default":0,"VCIFgtx8PqLPKqlv":3},"_stats":{"systemId":"swade","systemVersion":"2.1.7","coreVersion":"10.291","createdTime":1671037527330,"modifiedTime":1671470612586,"lastModifiedBy":"VCIFgtx8PqLPKqlv"}},{"folder":"owDBNhFlMteWJgQJ","name":"Arcane Background: Crystal Channeling","type":"edge","img":"icons/magic/symbols/star-solid-gold.webp","system":{"description":"<p>You are equipped with a harness. You are able to take ranks in the Crystal Channeling skill, which is based on Spirit. See page 56 for the full details. All Syn Agents get this Edge for free.</p>","notes":"","additionalStats":{},"favorite":false,"category":"Background Edges","isArcaneBackground":true,"requirements":{"value":"Novice"},"source":"Crystal Heart - Player Guide"},"effects":[],"sort":100000,"ownership":{"default":0,"VCIFgtx8PqLPKqlv":3},"flags":{"core":{"sourceId":"Item.WYe2VqMT92ohrsZP"}},"_stats":{"systemId":"swade","systemVersion":"2.1.7","coreVersion":"10.291","createdTime":1669643640177,"modifiedTime":1671037549930,"lastModifiedBy":"VCIFgtx8PqLPKqlv"},"_id":"oE5LVanQYQx34jBd"},{"name":"Silent Thought","type":"gear","img":"modules/swade-crystal-heart-starter/assets/crystals/Crystal-Silent-Thought.webp","system":{"description":"<p><strong>Description:</strong> Yellow, thin, shaped like an hourglass. Originally named Castle Cravin, after the place it was dug out of, it earned the nickname Good Fellow in testing and usage. After being lost and found again, it was renamed Silent Thought.</p><p><strong>Theme:</strong> Receive mental signals and surface thoughts from conscious beings, almost like a mental antenna. </p><p><em>Detect basic urges and simple mental states, such as reading the mood of a chicken.</em></p><p><strong>Benefit:</strong> Thanks to your increased empathy, you gain the Charismatic Edge.</p><p>@UUID[Compendium.swade-core-rules.swade-edges.3jFwJEuUKJJ7YLgz]{Charismatic}</p><p><strong>Disposition:</strong> You’re easily distracted because of the many mental glimpses you receive, resulting in -2 to Notice checks.</p><p><strong>Deep Connection</strong> (<em>empathy</em>, -1). You focus on receiving a specific person’s surface thoughts. This power is resisted with Notice instead of Spirit to notice your somewhat strange behavior (staring at them intently, mouthing their words slightly as they speak). If they begin to suspect you are using a power on them, the sudden self-reflecting realization forces you to lose focus on them, ending it.</p><p>@UUID[Compendium.swade-core-rules.swade-powers.oKfNj3eBIdd0Z5yB]{Empathy}</p><p><strong>Mental Antenna</strong> (<em>mind reading</em>, -1) You try to attune yourself to another person’s brainwaves to receive their thoughts. The target is not aware of this. A raise on the check allows you to gain a second truthful answer or more details.</p><p>@UUID[Compendium.swade-core-rules.swade-powers.WGoNElsK8VbvIma8]{Mind Link}</p><p><strong>Mental Vortex</strong> (<em>divination</em>, -3) Opening yourself to the world at large, you receive thoughts and memories—maybe even echoes from the deceased—in an almost overpowering surge. Sitting as quietly as you can for five minutes, you sift through the cacophony in search of useful information.</p><p>@UUID[Compendium.swade-core-rules.swade-powers.AF3ASpvByLYRkdkh]{Divination}</p><p></p>","notes":"","additionalStats":{},"quantity":1,"weight":0,"price":0,"equippable":true,"equipStatus":3,"isArcaneDevice":true,"arcaneSkillDie":{"sides":4,"modifier":0},"powerPoints":{"value":null,"max":null},"isVehicular":false,"mods":1,"actions":{"skill":"Crystal Channeling","skillMod":"","dmgMod":"","additional":{}},"favorite":true,"category":"Novice Crystals","isAmmo":false,"source":""},"effects":[{"label":"Silent Thought","icon":"modules/swade-crystal-heart/assets/crystals/Crystal-Silent-Thought.webp","duration":{"rounds":null,"startTime":0,"seconds":null,"combat":null,"turns":null,"startRound":null,"startTurn":null},"flags":{"swade":{"expiration":null}},"changes":[{"key":"@Skill{Notice}[system.die.modifier]","mode":2,"value":"-2","priority":null}],"transfer":true,"_id":"5nQEUqUryLbZxvVZ","disabled":false,"origin":null,"tint":null}],"folder":"KwL2i8tT9PRcrEjU","sort":100000,"ownership":{"default":0,"VCIFgtx8PqLPKqlv":3},"flags":{"swade":{"embeddedPowers":[["nLuir3nLaglD4M8P",{"_id":"AF3ASpvByLYRkdkh","effects":[],"flags":{"cf":{"color":"#8c160f","id":"temp_2ffpxea92cv","path":"Powers"},"core":{"sourceId":"Compendium.swade-core-rules.swade-powers.AF3ASpvByLYRkdkh"}},"folder":null,"img":"systems/swade/assets/icons/power.svg","name":"Divination","sort":0,"type":"power","system":{"description":"<article class=\"swade-core\">\n<p><em><span class=\"fontstyle0\">Divination </span></em><span class=\"fontstyle2\">allows the caster to contact otherworldly beings or forces to gain information.</span></p>\n<p><span class=\"fontstyle2\">Preparing the spell takes one uninterrupted minute, and must be done at a site relevant to the spirit being contacted. If speaking with a ghost, for example, the conversation should occur at its grave, where it was killed, or with a personal object in hand. Oracles may only be engaged at temples, fire spirits near open flames, and so on.</span></p>\n<p><span class=\"fontstyle2\">After the preparation time is complete, the caster makes her arcane skill roll. @UUID[Compendium.swade-core-rules.swade-rules.swadecor03rules0.JournalEntryPage.03traitrolls0000]{Success} allows her to engage with the spirit and ask it any questions she might have. The entity must answer to the best of its ability, as directly or ambiguously as the Game Master feels fits the spirit&rsquo;s personality and the feel of her campaign. A @UUID[Compendium.swade-core-rules.swade-rules.swadecor03rules0.JournalEntryPage.03traitrolls0000]{raise} means the spirit is more helpful, knowledgeable, or direct than usual (whether it wants to be or not). </span><span class=\"fontstyle2\">The type of entity contacted determines how it communicates. Spirits in the netherworld tend to know only those things pertinent to their life and death&mdash;and they may not even know the identity of their killer if the person was masked or unseen.</span></p>\n<p><span class=\"fontstyle2\">Likewise, nature spirits know only about their local environs, demons know only about the affairs they&rsquo;re directly involved in, and so on. The Game Master must decide what the entity knows based on its background, origin, and personality. </span><span class=\"fontstyle2\">Once a spirit has been conversed with it may not be contacted again for 24 hours. The Game Master may extend this to a community of spirits in a single place, such as water spirits in a babbling brook or nameless spirits in a mass grave.</span></p>\n</article>","notes":"","additionalStats":{},"actions":{"skill":"","skillMod":"","dmgMod":"","additional":{}},"bonusDamageDie":6,"favorite":false,"templates":{"cone":false,"stream":false,"small":false,"medium":false,"large":false},"rank":"Heroic","pp":"5","damage":"","range":"Self","duration":"A brief conversation of about five minutes","trapping":"Seances, prayers, demonic interrogation.","arcane":"","ap":0,"modifiers":[],"equippable":false,"equipped":false,"skill":""},"ownership":{"7ntJSLixH2Phv2jV":3,"default":0},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}}],["tjTSTPlDV2xekeOE",{"_id":"WGoNElsK8VbvIma8","effects":[],"flags":{"cf":{"color":"#8c160f","id":"temp_2ffpxea92cv","path":"Powers"},"core":{"sourceId":"Compendium.swade-core-rules.swade-powers.WGoNElsK8VbvIma8"}},"folder":null,"img":"systems/swade/assets/icons/power.svg","name":"Mind Link","sort":0,"type":"power","system":{"description":"<article class=\"swade-core\">\n<p><em><span class=\"fontstyle0\">Mind link </span></em><span class=\"fontstyle2\">creates a telepathic connection between two individuals (which does not necessarily have to include the caster). The link accommodates only willing subjects and communication&mdash;thoughts that aren&rsquo;t consciously transmitted aren&rsquo;t relayed.</span></p>\n<p><span class=\"fontstyle2\">Once @Compendium[swade-core-rules.swade-rules.Trait Rolls]{activated}, the @UUID[Compendium.swade-core-rules.swade-rules.swadecor02gear00.JournalEntryPage.02range000000000]{Range}</span><span class=\"fontstyle2\"> between all linked minds is one mile, or five with a @Compendium[swade-core-rules.swade-rules.Trait Rolls]{raise}.</span></p>\n<p><span class=\"fontstyle2\">If any of the linked characters suffers a @Compendium[swade-core-rules.swade-rules.Damage Effects]{Wound}</span><span class=\"fontstyle2\">, all others must make a @UUID[Compendium.swade-core-rules.swade-rules.swadecor01charac.JournalEntryPage.01traits00000000]{Smarts} roll or be @UUID[Compendium.swade-core-rules.swade-rules.swadecor03rules0.JournalEntryPage.03damageeffect00]{Shaken} </span>(this cannot cause a <span class=\"fontstyle2\">@Compendium[swade-core-rules.swade-rules.Damage Effects]{Wound}</span>).</p>\n<p><span class=\"fontstyle2\">The speed of communication is that of normal speech, but with a&nbsp;</span>@UUID[Compendium.swade-core-rules.swade-rules.swadecor03rules0.JournalEntryPage.03traitrolls0000]{raise}&nbsp;members may communicate up to 30 seconds or so of speech on a single combat turn.</p>\n<p><strong>Modifiers</strong></p>\n<ul>\n<li><strong>Additional Recipients (+1):</strong> The power may affect additional individuals for 1 additonal @UUID[Compendium.swade-core-rules.swade-rules.swadecor05powers.JournalEntryPage.05arcanebackgr00]{Power Point} each.&nbsp;</li>\n</ul>\n</article>","notes":"","additionalStats":{},"actions":{"skill":"","skillMod":"","dmgMod":"","additional":{}},"bonusDamageDie":6,"favorite":false,"templates":{"cone":false,"stream":false,"small":false,"medium":false,"large":false},"rank":"Novice","pp":"1","damage":"","range":"Sm","duration":"30 Minutes","trapping":"Headpieces, talismans, of Tao or other gods, crystals","arcane":"","ap":0,"modifiers":[],"equippable":false,"equipped":false,"skill":""},"ownership":{"7ntJSLixH2Phv2jV":3,"default":0},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}}],["IVYdPjtrGOAOWG2P",{"_id":"oKfNj3eBIdd0Z5yB","effects":[],"flags":{"cf":{"color":"#8c160f","id":"temp_2ffpxea92cv","path":"Powers"},"core":{"sourceId":"Compendium.swade-core-rules.swade-powers.oKfNj3eBIdd0Z5yB"}},"folder":null,"img":"systems/swade/assets/icons/power.svg","name":"Empathy","sort":0,"type":"power","system":{"description":"<article class=\"swade-core\">\n<p><span class=\"fontstyle0\">The caster forms an emotional bond between himself and the target with a successful @Compendium[swade-core-rules.swade-rules.Arcane Backgrounds]{arcane skill} vs @UUID[Compendium.swade-core-rules.swade-rules.swadecor01charac.JournalEntryPage.01traits00000000]{Spirit} roll. He knows the target&rsquo;s emotional state and most basic surface</span>&nbsp; t<span class=\"fontstyle0\">houghts, and gains +1 (+2 with a @Compendium[swade-core-rules.swade-rules.Trait Rolls]{raise}) to @Compendium[swade-core-rules.swade-skills.Intimidation]{Intimidation}, @Compendium[swade-core-rules.swade-skills.Persuasion]{Persuasion}, @Compendium[swade-core-rules.swade-skills.Performance]{Performance}, or @Compendium[swade-core-rules.swade-skills.Taunt]{Taunt} rolls against him.</span></p>\n<p><em><span class=\"fontstyle0\">Empathy </span></em><span class=\"fontstyle2\">also works on animals, adding +2 to @Compendium[swade-core-rules.swade-skills.Riding]{Riding} or other rolls used to interact with the creature</span> .&nbsp;</p>\n<p>&nbsp;</p>\n</article>","notes":"","additionalStats":{},"actions":{"skill":"","skillMod":"","dmgMod":"","additional":{}},"bonusDamageDie":6,"favorite":false,"templates":{"cone":false,"stream":false,"small":false,"medium":false,"large":false},"rank":"Novice","pp":"1","damage":"","range":"Sm","duration":"5","trapping":"Concentration, a warm or sympathetic smile, exchanging a pleasantry","arcane":"","ap":0,"modifiers":[],"equippable":false,"equipped":false,"skill":""},"ownership":{"7ntJSLixH2Phv2jV":3,"default":0},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}}]]},"core":{"sourceId":"Item.msOqhumlGtC9ZEjx"}},"_stats":{"systemId":"swade","systemVersion":"2.1.7","coreVersion":"10.291","createdTime":1669419714702,"modifiedTime":1671479326269,"lastModifiedBy":"VCIFgtx8PqLPKqlv"},"_id":"QkcELF8joa7JxBoG"},{"folder":"6xeflX6PzIFvBs8k","name":"Engineering","type":"skill","img":"icons/commodities/tech/blueprint.webp","system":{"description":"<p>This skill combines Electronics and Repair into one. It’s about practical application of technical knowledge, mainly to construct and repair devices.</p>","notes":"","additionalStats":{},"attribute":"smarts","isCoreSkill":false,"die":{"sides":6,"modifier":0},"wild-die":{"sides":6},"source":""},"effects":[],"sort":0,"ownership":{"default":0,"VCIFgtx8PqLPKqlv":3},"flags":{"core":{"sourceId":"Item.UMT7TQLJJGoxZMfu"}},"_stats":{"systemId":"swade","systemVersion":"2.1.7","coreVersion":"10.291","createdTime":1669641169083,"modifiedTime":1671037967076,"lastModifiedBy":"VCIFgtx8PqLPKqlv"},"_id":"ztz9WeHiLaMESfHL"},{"folder":"6xeflX6PzIFvBs8k","name":"Shooting","type":"skill","img":"icons/weapons/guns/gun-topbarrel.webp","system":{"description":"<p>As with Fighting, Syn provides Agents with several types of weaponry but encourages specialization in a chosen form. It’s expected that all Syn Agents who don’t specialize at throwing things take Shooting to have some ranged combat ability.</p>","notes":"","additionalStats":{},"attribute":"agility","isCoreSkill":false,"die":{"sides":8,"modifier":0},"wild-die":{"sides":6},"source":""},"effects":[],"sort":0,"ownership":{"default":0,"VCIFgtx8PqLPKqlv":3},"flags":{"core":{"sourceId":"Item.iPcfOwdAYOCvZNMy"}},"_stats":{"systemId":"swade","systemVersion":"2.1.7","coreVersion":"10.291","createdTime":1669641169083,"modifiedTime":1671038009436,"lastModifiedBy":"VCIFgtx8PqLPKqlv"},"_id":"K8NqAS3ohTM7t9MS"},{"folder":"6xeflX6PzIFvBs8k","name":"Thievery","type":"skill","img":"icons/tools/hand/lockpicks-steel-grey.webp","system":{"description":"<p>When disabling traps, being quick and careful is what’s important.</p>","notes":"","additionalStats":{},"attribute":"agility","isCoreSkill":false,"die":{"sides":6,"modifier":0},"wild-die":{"sides":6},"source":""},"effects":[],"sort":0,"ownership":{"default":0,"VCIFgtx8PqLPKqlv":3},"flags":{"core":{"sourceId":"Item.QHwuhYSj6uF2Zcgs"}},"_stats":{"systemId":"swade","systemVersion":"2.1.7","coreVersion":"10.291","createdTime":1669641169083,"modifiedTime":1671038023971,"lastModifiedBy":"VCIFgtx8PqLPKqlv"},"_id":"xRIGPMCjJZzbN8bz"},{"_id":"ZrnvMIqfDfdECmgo","effects":[],"flags":{"cf":{"color":"#8c160f","id":"temp_d0zzw406xvs","path":"Hindrances"},"core":{"sourceId":"Compendium.swade-core-rules.swade-hindrances.IQgZkCUMklDKYV9x"}},"folder":null,"img":"systems/swade/assets/icons/hindrance.svg","name":"Outsider","sort":0,"type":"hindrance","system":{"description":"<article class=\"swade-core\">\n<p>(MINOR OR MAJOR)</p>\n<p><span class=\"fontstyle0\">In a society made up of only a few types of people, your hero isn&rsquo;t one of them. A Native American in a Western town, an alien in a scifi game of human marines, or a half-orc in a party of elves, dwarves, and humans are all examples. Locals are likely to raise prices on her, ignore pleas for help, and generally treat her as lower class.<br /></span></p>\n<p><span class=\"fontstyle0\">Outsiders subtract 2 from @Compendium[swade-core-rules.swade-skills.Persuasion]{Persuasion} @UUID[Compendium.swade-core-rules.swade-rules.swadecor03rules0.JournalEntryPage.03traitrolls0000]{rolls} made to influence those who aren&rsquo;t her own kind. The @UUID[Compendium.swade-core-rules.swade-rules.swadecor01charac.JournalEntryPage.01characters0000]{Major} version also means the character has few or no legal rights in the main campaign area. She might be a different species among xenophobes, the civilization may be cruel and unenlightened toward strangers, or she might even be an artificial intelligence whose sapience isn&rsquo;t acknowledged by the law.</span></p>\n</article>","notes":"","additionalStats":{},"favorite":false,"major":false,"source":""},"ownership":{"7ntJSLixH2Phv2jV":3,"default":0,"VCIFgtx8PqLPKqlv":3},"_stats":{"systemId":"swade","systemVersion":"2.1.7","coreVersion":"10.291","createdTime":1671038401548,"modifiedTime":1671038548212,"lastModifiedBy":"VCIFgtx8PqLPKqlv"}},{"_id":"dNTh52TuP9Sv2Nj2","effects":[],"flags":{"cf":{"color":"#8c160f","id":"temp_d0zzw406xvs","path":"Hindrances"},"core":{"sourceId":"Compendium.swade-core-rules.swade-hindrances.rkkO0IANAxTqK2K4"}},"folder":null,"img":"systems/swade/assets/icons/hindrance.svg","name":"Shamed (Because I stole it, she died)","sort":0,"type":"hindrance","system":{"description":"<article class=\"swade-core\">\n<p>(MINOR OR MAJOR)</p>\n<p><span class=\"fontstyle0\">Something haunts your adventurer. Maybe he made a vow he didn&rsquo;t keep. Maybe he was defeated in an honorable fight and ordered the death of his foe anyway for some greater principle. Maybe he isn&rsquo;t actually cowardly but once ran from a battle and left others to die. </span></p>\n<p><span class=\"fontstyle0\">As a @UUID[Compendium.swade-core-rules.swade-rules.swadecor01charac.JournalEntryPage.01characters0000]{Minor} @UUID[Compendium.swade-core-rules.swade-rules.swadecor01charac.JournalEntryPage.01hindrances0000]{Hindrance}, the shameful circumstances aren&rsquo;t generally known&mdash;it just haunts the hero. He might go out of his way, against all reason, not to repeat the mistake. Or he might give in to the same set of circumstances and hate himself for it. </span></p>\n<p><span class=\"fontstyle0\">As a @UUID[Compendium.swade-core-rules.swade-rules.swadecor01charac.JournalEntryPage.01characters0000]{Major} @UUID[Compendium.swade-core-rules.swade-rules.swadecor01charac.JournalEntryPage.01hindrances0000]{Hindrance}, his deed is wellknown&mdash;or at least known among those he cares about. The other player characters should be told the tale as soon as possible (preferably at character creation). If not, it should be revealed by @Compendium[swade-core-rules.swade-rules.Wild Cards and Extras]{nonplayer characters} at some point, and occasionally used against the hero.</span></p>\n</article>","notes":"","additionalStats":{},"favorite":false,"major":false,"source":""},"ownership":{"7ntJSLixH2Phv2jV":3,"default":0,"VCIFgtx8PqLPKqlv":3},"_stats":{"systemId":"swade","systemVersion":"2.1.7","coreVersion":"10.291","createdTime":1671038408130,"modifiedTime":1671038427909,"lastModifiedBy":"VCIFgtx8PqLPKqlv"}},{"_id":"3BqEj8VgJI4yDZbg","effects":[],"flags":{"cf":{"color":"#8c160f","id":"temp_d0zzw406xvs","path":"Hindrances"},"core":{"sourceId":"Compendium.swade-core-rules.swade-hindrances.cgKdIeU474Fy2LFh"}},"folder":null,"img":"systems/swade/assets/icons/hindrance.svg","name":"Ugly","sort":0,"type":"hindrance","system":{"description":"<article class=\"swade-core\">\n<p><span class=\"fontstyle0\">(MINOR OR MAJOR)</span></p>\n<p><span class=\"fontstyle0\">@Compendium[swade-core-rules.swade-edges.Attractive]{Attractive} people get all the breaks. This unfortunate individual isn&rsquo;t one of them. He subtracts 1 from his @Compendium[swade-core-rules.swade-skills.Persuasion]{Persuasion} @UUID[Compendium.swade-core-rules.swade-rules.swadecor03rules0.JournalEntryPage.03traitrolls0000]{rolls}, or 2 if taken as a @UUID[Compendium.swade-core-rules.swade-rules.swadecor01charac.JournalEntryPage.01characters0000]{Major} @UUID[Compendium.swade-core-rules.swade-rules.swadecor01charac.JournalEntryPage.01hindrances0000]{Hindrance}.</span></p>\n</article>","notes":"","additionalStats":{},"favorite":false,"major":false,"source":""},"ownership":{"7ntJSLixH2Phv2jV":3,"default":0,"VCIFgtx8PqLPKqlv":3},"_stats":{"systemId":"swade","systemVersion":"2.1.7","coreVersion":"10.291","createdTime":1671038412363,"modifiedTime":1671038590677,"lastModifiedBy":"VCIFgtx8PqLPKqlv"}},{"effects":[{"label":"Home Land: Fjordstad","icon":"modules/swade-crystal-heart/assets/syn_logo.webp","duration":{"rounds":null,"startTime":174,"seconds":null,"combat":null,"turns":null,"startRound":null,"startTurn":null},"flags":{"swade":{"expiration":null,"loseTurnOnHold":false}},"changes":[{"key":"system.attributes.smarts.die.sides","mode":2,"value":"2","priority":null}],"transfer":true,"_id":"nKdpcz8xHDEIPKTD","disabled":false,"origin":null,"tint":null}],"flags":{"cf":{"color":"#88140e","id":"temp_nqwhmt9cidk","path":"Races"},"core":{"sourceId":"Compendium.swade-core-rules.swade-races.jIfKZgCogcSu3SQz"},"swade":{"embeddedAbilities":[]}},"img":"icons/environment/people/group.webp","name":"Home Land: Fjordstad","type":"ability","system":{"description":"<h1><span style=\"font-family: Signika, sans-serif\">Home Land: Fjordstad</span></h1><blockquote><p><span style=\"font-family: Signika, sans-serif\"><strong style=\"box-sizing: border-box; user-select: text;\">Fjordstad: </strong>Smarts starting at d6. Fjordstadians are known as being cunning and clever, approaching most situations as puzzles to be solved.</span></p></blockquote><p><span style=\"font-family: Signika, sans-serif\">In Crystal Heart everyone is human (gaining a free Novice Edge as usual), and everyone comes from a specific Land.</span></p><p style=\"box-sizing: border-box; user-select: text; margin: 0.5em 0px; color: rgb(25, 24, 19); font-family: Signika, sans-serif; font-size: 14px; font-style: normal; font-variant-ligatures: normal; font-variant-caps: normal; font-weight: 400; letter-spacing: normal; orphans: 2; text-indent: 0px; text-transform: none; white-space: normal; widows: 2; word-spacing: 0px; -webkit-text-stroke-width: 0px; text-decoration-thickness: initial; text-decoration-style: initial; text-decoration-color: initial; text-align: start\"><span style=\"font-family: Signika, sans-serif\">The Five Lands represent the entirety of the known world. Each Land’s population is made up of various cultures, but for the most part, each Land has its own ethnicity. For more details on the world and each Land, see The Five Lands, page 80.</span></p><p style=\"box-sizing: border-box; user-select: text; margin: 0.5em 0px; color: rgb(25, 24, 19); font-family: Signika, sans-serif; font-size: 14px; font-style: normal; font-variant-ligatures: normal; font-variant-caps: normal; font-weight: 400; letter-spacing: normal; orphans: 2; text-indent: 0px; text-transform: none; white-space: normal; widows: 2; word-spacing: 0px; -webkit-text-stroke-width: 0px; text-decoration-thickness: initial; text-decoration-style: initial; text-decoration-color: initial; text-align: start\"><span style=\"font-family: Signika, sans-serif\">Your choice of home Land increases one of your attributes by one step, as a result of both biological and cultural causes. Your maximum attribute limit is also increased by one step, to d12+1. Your choice of home Land also determines the type of things you might know with a Common Knowledge check.</span></p><p>@UUID[JournalEntry.D4jWOJxhzc0mbpuA.JournalEntryPage.pkaLQGaYshQNHKhO]{Step 1: Home Land}</p><p><br class=\"Apple-interchange-newline\"></p><p></p>","notes":"","additionalStats":{},"favorite":false,"subtype":"special","grantsPowers":false,"source":""},"ownership":{"default":0,"VCIFgtx8PqLPKqlv":3},"_stats":{"systemId":"swade","systemVersion":"2.1.7","coreVersion":"10.291","createdTime":1670866849578,"modifiedTime":1671038479204,"lastModifiedBy":"VCIFgtx8PqLPKqlv"},"folder":"TwNtLitVD0K3kg7V","sort":100000,"_id":"svBvcOe2E5uv62jq"},{"_id":"XLM3vYaLpv36aLKc","effects":[],"flags":{"cf":{"color":"#000000","id":"temp_neny3715hqg","path":"Edges#/CF_SEP/Background Edges"},"core":{"sourceId":"Compendium.swade-core-rules.swade-edges.3jFwJEuUKJJ7YLgz"}},"folder":null,"img":"systems/swade/assets/icons/edge.svg","name":"Charismatic (Silent Thought)","sort":0,"type":"edge","system":{"description":"<article class=\"swade-core\">\n<p><span class=\"fontstyle0\">Your hero is likable for some reason. She may be trustworthy or kind, or might just exude confidence and goodwill. You get one free reroll on @Compendium[swade-core-rules.swade-skills.Persuasion]{Persuasion} rolls.</span></p>\n</article>","notes":"","additionalStats":{},"favorite":false,"category":"","isArcaneBackground":false,"requirements":{"value":"Novice, Spirit d8+"},"source":""},"ownership":{"7ntJSLixH2Phv2jV":3,"default":0,"VCIFgtx8PqLPKqlv":3},"_stats":{"systemId":"swade","systemVersion":"2.1.7","coreVersion":"10.291","createdTime":1671039492950,"modifiedTime":1671039500476,"lastModifiedBy":"VCIFgtx8PqLPKqlv"}},{"_id":"cXCyXIn4XSL0ISGX","effects":[],"flags":{"cf":{"color":"#8c160f","id":"temp_2ffpxea92cv","path":"Powers"},"core":{"sourceId":"Compendium.swade-core-rules.swade-powers.oKfNj3eBIdd0Z5yB"}},"folder":null,"img":"systems/swade/assets/icons/power.svg","name":"Empathy","sort":0,"type":"power","system":{"description":"<article class=\"swade-core\">\n<p><span class=\"fontstyle0\">The caster forms an emotional bond between himself and the target with a successful @Compendium[swade-core-rules.swade-rules.Arcane Backgrounds]{arcane skill} vs @UUID[Compendium.swade-core-rules.swade-rules.swadecor01charac.JournalEntryPage.01traits00000000]{Spirit} roll. He knows the target&rsquo;s emotional state and most basic surface</span>&nbsp; t<span class=\"fontstyle0\">houghts, and gains +1 (+2 with a @Compendium[swade-core-rules.swade-rules.Trait Rolls]{raise}) to @Compendium[swade-core-rules.swade-skills.Intimidation]{Intimidation}, @Compendium[swade-core-rules.swade-skills.Persuasion]{Persuasion}, @Compendium[swade-core-rules.swade-skills.Performance]{Performance}, or @Compendium[swade-core-rules.swade-skills.Taunt]{Taunt} rolls against him.</span></p>\n<p><em><span class=\"fontstyle0\">Empathy </span></em><span class=\"fontstyle2\">also works on animals, adding +2 to @Compendium[swade-core-rules.swade-skills.Riding]{Riding} or other rolls used to interact with the creature</span> .&nbsp;</p>\n<p>&nbsp;</p>\n</article>","notes":"","additionalStats":{},"actions":{"skill":"Crystal Channeling","skillMod":"","dmgMod":"","additional":{}},"bonusDamageDie":6,"favorite":false,"templates":{"cone":false,"stream":false,"small":false,"medium":false,"large":false},"rank":"Novice","pp":"1","damage":"","range":"Sm","duration":"5","trapping":"Concentration, a warm or sympathetic smile, exchanging a pleasantry","arcane":"","ap":0,"modifiers":[],"equippable":false,"equipped":false,"skill":"","source":""},"ownership":{"7ntJSLixH2Phv2jV":3,"default":0,"VCIFgtx8PqLPKqlv":3},"_stats":{"systemId":"swade","systemVersion":"2.1.7","coreVersion":"10.291","createdTime":1671039515117,"modifiedTime":1671039537449,"lastModifiedBy":"VCIFgtx8PqLPKqlv"}},{"_id":"kdazzYN5tlJXzLky","effects":[],"flags":{"cf":{"color":"#8c160f","id":"temp_2ffpxea92cv","path":"Powers"},"core":{"sourceId":"Compendium.swade-core-rules.swade-powers.WGoNElsK8VbvIma8"}},"folder":null,"img":"systems/swade/assets/icons/power.svg","name":"Mind Link","sort":0,"type":"power","system":{"description":"<article class=\"swade-core\">\n<p><em><span class=\"fontstyle0\">Mind link </span></em><span class=\"fontstyle2\">creates a telepathic connection between two individuals (which does not necessarily have to include the caster). The link accommodates only willing subjects and communication&mdash;thoughts that aren&rsquo;t consciously transmitted aren&rsquo;t relayed.</span></p>\n<p><span class=\"fontstyle2\">Once @Compendium[swade-core-rules.swade-rules.Trait Rolls]{activated}, the @UUID[Compendium.swade-core-rules.swade-rules.swadecor02gear00.JournalEntryPage.02range000000000]{Range}</span><span class=\"fontstyle2\"> between all linked minds is one mile, or five with a @Compendium[swade-core-rules.swade-rules.Trait Rolls]{raise}.</span></p>\n<p><span class=\"fontstyle2\">If any of the linked characters suffers a @Compendium[swade-core-rules.swade-rules.Damage Effects]{Wound}</span><span class=\"fontstyle2\">, all others must make a @UUID[Compendium.swade-core-rules.swade-rules.swadecor01charac.JournalEntryPage.01traits00000000]{Smarts} roll or be @UUID[Compendium.swade-core-rules.swade-rules.swadecor03rules0.JournalEntryPage.03damageeffect00]{Shaken} </span>(this cannot cause a <span class=\"fontstyle2\">@Compendium[swade-core-rules.swade-rules.Damage Effects]{Wound}</span>).</p>\n<p><span class=\"fontstyle2\">The speed of communication is that of normal speech, but with a&nbsp;</span>@UUID[Compendium.swade-core-rules.swade-rules.swadecor03rules0.JournalEntryPage.03traitrolls0000]{raise}&nbsp;members may communicate up to 30 seconds or so of speech on a single combat turn.</p>\n<p><strong>Modifiers</strong></p>\n<ul>\n<li><strong>Additional Recipients (+1):</strong> The power may affect additional individuals for 1 additonal @UUID[Compendium.swade-core-rules.swade-rules.swadecor05powers.JournalEntryPage.05arcanebackgr00]{Power Point} each.&nbsp;</li>\n</ul>\n</article>","notes":"","additionalStats":{},"actions":{"skill":"Crystal Channeling","skillMod":"","dmgMod":"","additional":{}},"bonusDamageDie":6,"favorite":false,"templates":{"cone":false,"stream":false,"small":false,"medium":false,"large":false},"rank":"Novice","pp":"1","damage":"","range":"Sm","duration":"30 Minutes","trapping":"Headpieces, talismans, of Tao or other gods, crystals","arcane":"","ap":0,"modifiers":[],"equippable":false,"equipped":false,"skill":"","source":""},"ownership":{"7ntJSLixH2Phv2jV":3,"default":0,"VCIFgtx8PqLPKqlv":3},"_stats":{"systemId":"swade","systemVersion":"2.1.7","coreVersion":"10.291","createdTime":1671039517245,"modifiedTime":1671039541776,"lastModifiedBy":"VCIFgtx8PqLPKqlv"}},{"_id":"RAV7eKFC6cg9VZj5","effects":[],"flags":{"cf":{"color":"#8c160f","id":"temp_2ffpxea92cv","path":"Powers"},"core":{"sourceId":"Compendium.swade-core-rules.swade-powers.AF3ASpvByLYRkdkh"}},"folder":null,"img":"systems/swade/assets/icons/power.svg","name":"Divination","sort":0,"type":"power","system":{"description":"<article class=\"swade-core\">\n<p><em><span class=\"fontstyle0\">Divination </span></em><span class=\"fontstyle2\">allows the caster to contact otherworldly beings or forces to gain information.</span></p>\n<p><span class=\"fontstyle2\">Preparing the spell takes one uninterrupted minute, and must be done at a site relevant to the spirit being contacted. If speaking with a ghost, for example, the conversation should occur at its grave, where it was killed, or with a personal object in hand. Oracles may only be engaged at temples, fire spirits near open flames, and so on.</span></p>\n<p><span class=\"fontstyle2\">After the preparation time is complete, the caster makes her arcane skill roll. @UUID[Compendium.swade-core-rules.swade-rules.swadecor03rules0.JournalEntryPage.03traitrolls0000]{Success} allows her to engage with the spirit and ask it any questions she might have. The entity must answer to the best of its ability, as directly or ambiguously as the Game Master feels fits the spirit&rsquo;s personality and the feel of her campaign. A @UUID[Compendium.swade-core-rules.swade-rules.swadecor03rules0.JournalEntryPage.03traitrolls0000]{raise} means the spirit is more helpful, knowledgeable, or direct than usual (whether it wants to be or not). </span><span class=\"fontstyle2\">The type of entity contacted determines how it communicates. Spirits in the netherworld tend to know only those things pertinent to their life and death&mdash;and they may not even know the identity of their killer if the person was masked or unseen.</span></p>\n<p><span class=\"fontstyle2\">Likewise, nature spirits know only about their local environs, demons know only about the affairs they&rsquo;re directly involved in, and so on. The Game Master must decide what the entity knows based on its background, origin, and personality. </span><span class=\"fontstyle2\">Once a spirit has been conversed with it may not be contacted again for 24 hours. The Game Master may extend this to a community of spirits in a single place, such as water spirits in a babbling brook or nameless spirits in a mass grave.</span></p>\n</article>","notes":"","additionalStats":{},"actions":{"skill":"Crystal Channeling","skillMod":"","dmgMod":"","additional":{}},"bonusDamageDie":6,"favorite":false,"templates":{"cone":false,"stream":false,"small":false,"medium":false,"large":false},"rank":"Heroic","pp":"5","damage":"","range":"Self","duration":"A brief conversation of about five minutes","trapping":"Seances, prayers, demonic interrogation.","arcane":"","ap":0,"modifiers":[],"equippable":false,"equipped":false,"skill":"","source":""},"ownership":{"7ntJSLixH2Phv2jV":3,"default":0,"VCIFgtx8PqLPKqlv":3},"_stats":{"systemId":"swade","systemVersion":"2.1.7","coreVersion":"10.291","createdTime":1671039520848,"modifiedTime":1671039545849,"lastModifiedBy":"VCIFgtx8PqLPKqlv"}},{"effects":[],"flags":{"cf":{"color":"#000000","id":"temp_21wmezu6j78","path":"Equipment#/CF_SEP/Natural/Improvised Weapons"},"core":{"sourceId":"Item.qEZeiXLRO6rm6fMX"}},"folder":null,"img":"icons/skills/melee/unarmed-punch-fist.webp","name":"Unarmed","sort":12500,"type":"weapon","system":{"description":"","notes":"","additionalStats":{},"quantity":1,"weight":null,"price":null,"equippable":true,"equipStatus":5,"isArcaneDevice":false,"arcaneSkillDie":{"sides":4,"modifier":0},"powerPoints":{},"isVehicular":false,"mods":1,"actions":{"skill":"Fighting","skillMod":"","dmgMod":"","additional":{}},"bonusDamageDie":6,"favorite":true,"templates":{"cone":false,"stream":false,"small":false,"medium":false,"large":false},"category":"","damage":"@str","range":"","rof":"0","ap":0,"parry":0,"minStr":"","shots":0,"currentShots":0,"ammo":"","autoReload":false,"trademark":0,"isHeavyWeapon":false,"equipped":true,"source":""},"ownership":{"7ntJSLixH2Phv2jV":3,"default":0,"VCIFgtx8PqLPKqlv":3},"_stats":{"systemId":"swade","systemVersion":"2.1.7","coreVersion":"10.291","createdTime":1670433648190,"modifiedTime":1671470768636,"lastModifiedBy":"VCIFgtx8PqLPKqlv"},"_id":"MlxyQAHC1a0NBfFD"}],"effects":[{"_id":"EW7v5sEX7awULht4","changes":[{"key":"system.initiative.hasQuick","mode":5,"value":"True","priority":null}],"disabled":false,"duration":{"startTime":174,"seconds":null,"combat":null,"rounds":null,"turns":null,"startRound":null,"startTurn":null},"flags":{},"icon":"/icons/svg/mystery-man.svg","label":"Quick","tint":null,"transfer":false,"origin":"Actor.MkADZ8FDpoYaUCIj.Item.RPZJHgg6MszsaQPW"},{"label":"Silent Thought","icon":"modules/swade-crystal-heart/assets/crystals/Crystal-Silent-Thought.webp","duration":{"rounds":null,"startTime":174,"seconds":null,"combat":null,"turns":null,"startRound":null,"startTurn":null},"flags":{"swade":{"expiration":null}},"changes":[{"key":"@Skill{Notice}[system.die.modifier]","mode":2,"value":"-2","priority":null}],"transfer":false,"_id":"kvkGj9ZWoGu0D0Yp","disabled":false,"origin":"Actor.MkADZ8FDpoYaUCIj.Item.QkcELF8joa7JxBoG","tint":null},{"label":"Outsider","icon":"modules/swade-crystal-heart/assets/syn_logo.webp","duration":{"rounds":null,"startTime":174,"seconds":null,"combat":null,"turns":null,"startRound":null,"startTurn":null},"flags":{"swade":{"expiration":null}},"changes":[{"key":"@Skill{Persuasion}[system.die.modifier]","mode":2,"value":"-1","priority":null}],"transfer":false,"_id":"xfdFtub5YhDr49Lm","disabled":false,"origin":null,"tint":null},{"label":"Ugly","icon":"modules/swade-crystal-heart/assets/syn_logo.webp","duration":{"rounds":null,"startTime":174,"seconds":null,"combat":null,"turns":null,"startRound":null,"startTurn":null},"flags":{"swade":{"expiration":null}},"changes":[{"key":"@Skill{Persuasion}[system.die.modifier]","mode":2,"value":"-1","priority":null}],"transfer":false,"_id":"baUTlRMfR75RPOzQ","disabled":false,"origin":null,"tint":null}],"sort":0,"ownership":{"default":0,"VCIFgtx8PqLPKqlv":3},"flags":{},"_stats":{"systemId":"swade","systemVersion":"2.1.7","coreVersion":"10.291","createdTime":1671037429861,"modifiedTime":1671479326269,"lastModifiedBy":"VCIFgtx8PqLPKqlv"},"_id":"MkADZ8FDpoYaUCIj"},{"folder":"zagAvnFOUHKAtTXR","name":"Agent Chel","type":"character","img":"modules/swade-crystal-heart-starter/assets/agents/AgentChel.webp","system":{"attributes":{"agility":{"die":{"sides":8,"modifier":0},"wild-die":{"sides":6}},"smarts":{"die":{"sides":8,"modifier":0},"wild-die":{"sides":6},"animal":false},"spirit":{"die":{"sides":6,"modifier":0},"wild-die":{"sides":6},"unShakeBonus":0},"strength":{"die":{"sides":6,"modifier":0},"wild-die":{"sides":6},"encumbranceSteps":0},"vigor":{"die":{"sides":6,"modifier":0},"wild-die":{"sides":6}}},"stats":{"speed":{"runningDie":6,"runningMod":0,"value":6},"toughness":{"value":0,"armor":0,"modifier":0},"parry":{"value":0,"modifier":0},"size":0},"details":{"autoCalcToughness":true,"autoCalcParry":true,"archetype":"Zingama","appearance":"","notes":"","goals":"","biography":{"value":"<p>Even when she was a child, Chel was all action, no-nonsense. She used to feed her pet zapacha various fruits, just to see the creature’s reaction. After accidently poisoning her beloved Shmoop- Shmoop, she stopped her experiments immediately - only to move on to study the effect of sudden floods on ant hills. She stopped doing that as well, after someone told her the ants aren’t exactly thrilled.</p><p>Having both an analytic mind and a big heart is hard for Chel, especially since, despite caring deeply about all living things, she doesn’t seem to get them. Turning to Syn in hope of one day finding a Crystal that will provide her with some enlightenment, she spends her time as an efficient medic - although somewhat lacking in bedside manners.</p>"},"species":{"name":"Human"},"currency":0,"wealth":{"die":6,"modifier":0,"wild-die":6},"conviction":{"value":0,"active":false}},"powerPoints":{"general":{"value":0,"max":0}},"fatigue":{"value":0,"min":0,"max":2},"wounds":{"value":0,"min":0,"max":3,"ignored":0},"advances":{"mode":"expanded","value":0,"rank":"Novice","details":"","list":[]},"bennies":{"value":3,"max":3},"status":{"isShaken":false,"isDistracted":false,"isVulnerable":false,"isStunned":false,"isEntangled":false,"isBound":false,"isIncapacitated":false},"initiative":{"hasHesitant":false,"hasLevelHeaded":false,"hasImpLevelHeaded":false,"hasQuick":false},"additionalStats":{},"wildcard":true},"prototypeToken":{"name":"Chel","displayName":50,"actorLink":true,"texture":{"src":"modules/swade-crystal-heart-starter/assets/agents/AgentChel.webp","scaleX":1,"scaleY":1,"offsetX":0,"offsetY":0,"rotation":0,"tint":null},"width":1,"height":1,"lockRotation":false,"rotation":0,"alpha":1,"disposition":1,"displayBars":50,"bar1":{"attribute":"wounds"},"bar2":{"attribute":"fatigue"},"light":{"alpha":0.5,"angle":360,"bright":0,"color":null,"coloration":1,"dim":0,"attenuation":0.5,"luminosity":0.5,"saturation":0,"contrast":0,"shadows":0,"animation":{"type":null,"speed":5,"intensity":5,"reverse":false},"darkness":{"min":0,"max":1}},"sight":{"enabled":true,"range":3,"angle":360,"visionMode":"basic","color":null,"attenuation":0.1,"brightness":0,"saturation":0,"contrast":0},"detectionModes":[],"flags":{},"randomImg":false},"items":[{"name":"Common Knowledge","type":"skill","img":"icons/sundries/books/book-open-red.webp","system":{"description":"<p>With communications technology still in its infancy, people aren’t very knowledgeable about most of their Land, to say nothing of other Lands. You might be required to roll to see if you remember or are aware of specific details, such as customs (what people tend to celebrate, when, why, and how), dangers (hazardous weather conditions, fauna and flora, and where and when they might be encountered) and history (major political upheavals and wars, people in power, recent natural disasters).</p><p>Rolling Common Knowledge on Lands other than your own is usually done with a -1 penalty on the check. History tends to be foggy earlier than 300 years ago, so checks regarding those days are made with a -1 penalty. The Bygone Age is beyond Common Knowledge.</p><p>You might know some specialized information, thanks to your upbringing. Based on your Land, the following topics can be considered Common Knowledge to you, instead of Academics:</p><p>Bogovia: Farming techniques and diseases.</p><p>Fjordstad: Scientific principles, mechanics.</p><p>The Islands: Trade and sailing.</p><p>Maseia: Animal husbandry and weather.</p><p>Zingama: Sociology and narcotics.</p>","notes":"","additionalStats":{},"attribute":"smarts","isCoreSkill":true,"die":{"sides":4,"modifier":0},"wild-die":{"sides":6},"source":""},"effects":[],"flags":{},"_stats":{"systemId":"swade","systemVersion":"2.1.5","coreVersion":"10.290","createdTime":1669641169083,"modifiedTime":1669755985555,"lastModifiedBy":"VCIFgtx8PqLPKqlv"},"_id":"2sGQ1D7lZeh5tgFO","folder":null,"sort":0,"ownership":{"default":0,"VCIFgtx8PqLPKqlv":3}},{"name":"Crystal Channeling","type":"skill","img":"icons/commodities/gems/pearl-water.webp","system":{"description":"<p>The arcane skill used for Arcane Background (Crystal Channeling), with which one activates their socketed Crystal’s powers. Channeling is a mental exercise practiced with biofeedback techniques. See page 56 for the full details.</p>","notes":"","additionalStats":{},"attribute":"spirit","isCoreSkill":true,"die":{"sides":8,"modifier":0},"wild-die":{"sides":6},"source":""},"effects":[],"flags":{},"_stats":{"systemId":"swade","systemVersion":"2.1.7","coreVersion":"10.291","createdTime":1669419905661,"modifiedTime":1671038738184,"lastModifiedBy":"VCIFgtx8PqLPKqlv"},"_id":"6c0pHP94LmVmLldh","folder":null,"sort":0,"ownership":{"default":0,"VCIFgtx8PqLPKqlv":3}},{"name":"Notice","type":"skill","img":"icons/magic/perception/eye-slit-orange.webp","system":{"description":"<p>When seeing a Crystal in action, feral or otherwise, a raise on a Notice check to study its manifestation can give a clue to its theme.</p>","notes":"","additionalStats":{},"attribute":"smarts","isCoreSkill":true,"die":{"sides":8,"modifier":0},"wild-die":{"sides":6},"source":""},"effects":[],"flags":{},"_stats":{"systemId":"swade","systemVersion":"2.1.7","coreVersion":"10.291","createdTime":1669641169083,"modifiedTime":1671038765081,"lastModifiedBy":"VCIFgtx8PqLPKqlv"},"_id":"7l2ugxHNXKDAZPjX","folder":null,"sort":0,"ownership":{"default":0,"VCIFgtx8PqLPKqlv":3}},{"name":"Persuasion","type":"skill","img":"icons/skills/social/diplomacy-handshake.webp","system":{"description":"<p>Nobody in the Five Lands is indifferent toward you—see page 82 for the Setting Rule Everyone Has an Opinion About Syn. Persuasion can be used to Requisition equipment and services from Syn (see page 36).</p><p>Networking: Instead of paying with money an Agent can pay with Requisition, gaining +2 per point spent.</p>","notes":"","additionalStats":{},"attribute":"spirit","isCoreSkill":true,"die":{"sides":4,"modifier":0},"wild-die":{"sides":6},"source":""},"effects":[],"flags":{},"_stats":{"systemId":"swade","systemVersion":"2.1.5","coreVersion":"10.290","createdTime":1669641169083,"modifiedTime":1669755985557,"lastModifiedBy":"VCIFgtx8PqLPKqlv"},"_id":"DzncQlHd3qvGKegB","folder":null,"sort":0,"ownership":{"default":0,"VCIFgtx8PqLPKqlv":3}},{"name":"Athletics","type":"skill","img":"icons/magic/control/buff-strength-muscle-damage.webp","system":{"description":"<p>All Agents receive basic physical training.</p>","notes":"","additionalStats":{},"attribute":"agility","isCoreSkill":true,"die":{"sides":4,"modifier":0},"wild-die":{"sides":6},"source":""},"effects":[],"flags":{},"_stats":{"systemId":"swade","systemVersion":"2.1.5","coreVersion":"10.290","createdTime":1669641169083,"modifiedTime":1669755985554,"lastModifiedBy":"VCIFgtx8PqLPKqlv"},"_id":"SgRFoXu9cJLcaSPL","folder":null,"sort":0,"ownership":{"default":0,"VCIFgtx8PqLPKqlv":3}},{"name":"Fighting","type":"skill","img":"icons/skills/melee/unarmed-punch-fist.webp","system":{"description":"<p>Syn Agents get to train with several melee weapons and also bare-handed, but most decide to focus on one or two weapons or styles (usually inspired by their home Land). This is encouraged, as Syn places a huge emphasis on personal development and uniqueness brought together under unified beliefs and goals.</p>","notes":"","additionalStats":{},"attribute":"agility","isCoreSkill":true,"die":{"sides":6,"modifier":0},"wild-die":{"sides":6},"source":""},"effects":[],"flags":{},"_stats":{"systemId":"swade","systemVersion":"2.1.7","coreVersion":"10.291","createdTime":1669641169083,"modifiedTime":1671038756304,"lastModifiedBy":"VCIFgtx8PqLPKqlv"},"_id":"jCVYj2QS90GUDgJi","folder":null,"sort":0,"ownership":{"default":0,"VCIFgtx8PqLPKqlv":3}},{"name":"Stealth","type":"skill","img":"icons/equipment/back/cloak-heavy-fur-blue.webp","system":{"description":"<p>The world is surprisingly filled with places people don’t want you to go to. On occasion, Agents even try to pretend they never went there.</p>","notes":"","additionalStats":{},"attribute":"agility","isCoreSkill":true,"die":{"sides":4,"modifier":0},"wild-die":{"sides":6},"source":""},"effects":[],"flags":{},"_stats":{"systemId":"swade","systemVersion":"2.1.7","coreVersion":"10.291","createdTime":1669641169083,"modifiedTime":1671038789201,"lastModifiedBy":"VCIFgtx8PqLPKqlv"},"_id":"lCpxk2gD9M0Jopm8","folder":null,"sort":0,"ownership":{"default":0,"VCIFgtx8PqLPKqlv":3}},{"name":"Unskilled","type":"skill","img":"systems/swade/assets/icons/skill.svg","_id":"caLuXf4cDLjRgiG1","system":{"description":"","notes":"","additionalStats":{},"attribute":"","isCoreSkill":false,"die":{"sides":4,"modifier":0},"wild-die":{"sides":6}},"effects":[],"folder":null,"sort":0,"ownership":{"default":0},"flags":{},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"folder":"T2n6BizHV2OMrzEh","name":"Syn suit","type":"armor","img":"icons/equipment/chest/breastplate-scale-grey.webp","system":{"description":"<p><strong>Requisition Modifier:</strong> 0</p><p><strong>Syn suit:</strong> Each Agent is issued 3 custom- made suits tailored to their measurements. The suit is made with reinforced fabric which provides one point of Armor, and has various pockets or sashes so the wearer can carry their favorite weapon with ease. The suit is also insulated, providing basic protection in harsh weather.</p>","notes":"","additionalStats":{},"quantity":1,"weight":8,"price":0,"equippable":false,"equipStatus":3,"isArcaneDevice":false,"arcaneSkillDie":{"sides":4,"modifier":0},"powerPoints":{},"actions":{"skill":"","skillMod":"","dmgMod":"","additional":{}},"favorite":true,"bonusDamageDie":6,"category":"","minStr":"d4","armor":1,"toughness":0,"isNaturalArmor":false,"isHeavyArmor":false,"locations":{"head":false,"torso":true,"arms":true,"legs":true},"source":""},"effects":[],"sort":106250,"ownership":{"default":0,"VCIFgtx8PqLPKqlv":3},"flags":{"core":{"sourceId":"Item.Ap927649wiKXqlmH"}},"_stats":{"systemId":"swade","systemVersion":"2.1.7","coreVersion":"10.291","createdTime":1669746177033,"modifiedTime":1671037500012,"lastModifiedBy":"VCIFgtx8PqLPKqlv"},"_id":"VV60wlYuHf6P1mAp"},{"folder":"TdmDkNwjyyhbxprQ","name":"Rapier Syn Sword","type":"weapon","img":"icons/weapons/swords/swords-short.webp","system":{"description":"<p><strong>Requisition Modifier:</strong> +1</p><p>Syn Sword: These statistics represent a generic sword, but it’s possible to Requisition some modifications. Any of the following can be added, up to twice each: </p><ul><li><p>AP 1;</p></li><li><p>Parry +1;</p></li><li><p> Reach 1; </p></li><li><p>increase damage die by one step. </p></li></ul><p>Each modification costs 1 point of Requisition (2 if taken a second time) which is spent after the roll. Having more than one modification increases the sword’s minimum Strength requirement to d8.<br></p>","notes":"","additionalStats":{},"quantity":1,"weight":2,"price":0,"equippable":false,"equipStatus":4,"isArcaneDevice":false,"arcaneSkillDie":{"sides":4,"modifier":0},"powerPoints":{},"isVehicular":false,"mods":1,"actions":{"skill":"Fighting","skillMod":"","dmgMod":"","additional":{}},"bonusDamageDie":6,"favorite":true,"templates":{"cone":false,"stream":false,"small":false,"medium":false,"large":false},"category":"Melee Weapons","damage":"@str+d4","range":"","rof":"1","ap":0,"parry":1,"minStr":"d6","shots":0,"currentShots":0,"ammo":"","autoReload":false,"trademark":0,"isHeavyWeapon":false,"source":""},"effects":[],"sort":106250,"ownership":{"default":0,"VCIFgtx8PqLPKqlv":3},"flags":{"core":{"sourceId":"Item.SCmfh5q8PhXXkjCO"}},"_stats":{"systemId":"swade","systemVersion":"2.1.7","coreVersion":"10.291","createdTime":1669746803734,"modifiedTime":1671038818159,"lastModifiedBy":"VCIFgtx8PqLPKqlv"},"_id":"HWXFdoOYx8Alkp5u"},{"folder":"eaS1icNJKU74FEGZ","name":"Syn Pistol","type":"weapon","img":"icons/weapons/guns/gun-topbarrel.webp","system":{"description":"<p><strong>Requisition Modifier:</strong> +1</p><p><strong>Syn Pistol:</strong> A simple revolver, reliable and light. Can be upgraded to an advanced, semi-automatic version for -2 Requisition, increasing the range to 12/24/48 and the number of shots to 10. A second upgrade, which must be taken after the first, increases the damage to 2d8 for -4 Requisition.</p>","notes":"","additionalStats":{},"quantity":1,"weight":4,"price":0,"equippable":false,"equipStatus":4,"isArcaneDevice":false,"arcaneSkillDie":{"sides":4,"modifier":0},"powerPoints":{},"isVehicular":false,"mods":1,"actions":{"skill":"Shooting","skillMod":"","dmgMod":"","additional":{}},"bonusDamageDie":6,"favorite":true,"templates":{"cone":false,"stream":false,"small":false,"medium":false,"large":false},"category":"R